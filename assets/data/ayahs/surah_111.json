[{"ayah_number":1,"text_arabic":"تَبَّتْ يَدَا أَبِي لَهَبٍ وَتَبَّ","surah_id":111},{"ayah_number":2,"text_arabic":"مَا أَغْنَى عَنْهُ مَالُهُ وَمَا كَسَبَ","surah_id":111},{"ayah_number":3,"text_arabic":"سَيَصْلَى نَارًا ذَاتَ لَهَبٍ","surah_id":111},{"ayah_number":4,"text_arabic":"وَامْرَأَتُهُ حَمَّالَةَ الْحَطَبِ","surah_id":111},{"ayah_number":5,"text_arabic":"فِي جِيدِهَا حَبْلٌ مِنْ مَسَدٍ","surah_id":111}]