[{"ayah_number":1,"text_arabic":"وَيْلٌ لِكُلِّ هُمَزَةٍ لُمَزَةٍ","surah_id":104},{"ayah_number":2,"text_arabic":"الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ","surah_id":104},{"ayah_number":3,"text_arabic":"يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ","surah_id":104},{"ayah_number":4,"text_arabic":"كَلَّا لَيُنْبَذَنَّ فِي الْحُطَمَةِ  وَمَا أَدْرَاكَ مَا الْحُطَمَةُ","surah_id":104,"covers_ayahs":"4-5"},{"ayah_number":6,"text_arabic":"نَارُ اللَّهِ الْمُوقَدَةُ","surah_id":104},{"ayah_number":7,"text_arabic":"الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ","surah_id":104},{"ayah_number":8,"text_arabic":"إِنَّهَا عَلَيْهِمْ مُؤْصَدَةٌ","surah_id":104},{"ayah_number":9,"text_arabic":"فِي عَمَدٍ مُمَدَّدَةٍ","surah_id":104}]