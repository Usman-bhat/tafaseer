[{"ayah_number":1,"text_arabic":"لِإِيلَافِ قُرَيْشٍ","surah_id":106},{"ayah_number":2,"text_arabic":"إِيلَافِهِمْ رِحْلَةَ الشِّتَاءِ وَالصَّيْفِ","surah_id":106},{"ayah_number":3,"text_arabic":"فَلْيَعْبُدُوا رَبَّ هَذَا الْبَيْتِ","surah_id":106},{"ayah_number":4,"text_arabic":"الَّذِي أَطْعَمَهُمْ مِنْ جُوعٍ وَآمَنَهُمْ مِنْ خَوْفٍ","surah_id":106}]