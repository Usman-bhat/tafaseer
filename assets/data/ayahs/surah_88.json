[{"ayah_number":1,"text_arabic":"هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ  وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ  عَامِلَةٌ نَاصِبَةٌ","surah_id":88,"covers_ayahs":"1-3"},{"ayah_number":4,"text_arabic":"تَصْلَى نَارًا حَامِيَةً","surah_id":88},{"ayah_number":5,"text_arabic":"تُسْقَى مِنْ عَيْنٍ آنِيَةٍ","surah_id":88},{"ayah_number":6,"text_arabic":"لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ","surah_id":88},{"ayah_number":7,"text_arabic":"لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ","surah_id":88},{"ayah_number":8,"text_arabic":"وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ","surah_id":88},{"ayah_number":9,"text_arabic":"لِسَعْيِهَا رَاضِيَةٌ","surah_id":88},{"ayah_number":10,"text_arabic":"فِي جَنَّةٍ عَالِيَةٍ","surah_id":88},{"ayah_number":11,"text_arabic":"لَا تَسْمَعُ فِيهَا لَاغِيَةً","surah_id":88},{"ayah_number":12,"text_arabic":"فِيهَا عَيْنٌ جَارِيَةٌ","surah_id":88},{"ayah_number":13,"text_arabic":"فِيهَا سُرُرٌ مَرْفُوعَةٌ","surah_id":88},{"ayah_number":14,"text_arabic":"وَأَكْوَابٌ مَوْضُوعَةٌ","surah_id":88},{"ayah_number":15,"text_arabic":"وَنَمَارِقُ مَصْفُوفَةٌ","surah_id":88},{"ayah_number":16,"text_arabic":"وَزَرَابِيُّ مَبْثُوثَةٌ","surah_id":88},{"ayah_number":17,"text_arabic":"أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ","surah_id":88},{"ayah_number":18,"text_arabic":"وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ","surah_id":88},{"ayah_number":19,"text_arabic":"وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ","surah_id":88},{"ayah_number":20,"text_arabic":"وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ","surah_id":88},{"ayah_number":21,"text_arabic":"فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ","surah_id":88},{"ayah_number":22,"text_arabic":"لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ","surah_id":88},{"ayah_number":23,"text_arabic":"إِلَّا مَنْ تَوَلَّى وَكَفَرَ  فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ","surah_id":88,"covers_ayahs":"23-24"},{"ayah_number":25,"text_arabic":"إِنَّ إِلَيْنَا إِيَابَهُمْ  ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ","surah_id":88,"covers_ayahs":"25-26"}]