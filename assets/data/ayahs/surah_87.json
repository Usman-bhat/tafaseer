[{"ayah_number":1,"text_arabic":"سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى  الَّذِي خَلَقَ فَسَوَّى  وَالَّذِي قَدَّرَ فَهَدَى  وَالَّذِي أَخْرَجَ الْمَرْعَى  فَجَعَلَهُ غُثَاءً أَحْوَى","surah_id":87,"covers_ayahs":"1-5"},{"ayah_number":6,"text_arabic":"سَنُقْرِئُكَ فَلَا تَنْسَى  إِلَّا مَا شَاءَ اللَّهُ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَى","surah_id":87,"covers_ayahs":"6-7"},{"ayah_number":8,"text_arabic":"وَنُيَسِّرُكَ لِلْيُسْرَى","surah_id":87},{"ayah_number":9,"text_arabic":"فَذَكِّرْ إِنْ نَفَعَتِ الذِّكْرَى","surah_id":87},{"ayah_number":10,"text_arabic":"سَيَذَّكَّرُ مَنْ يَخْشَى","surah_id":87},{"ayah_number":11,"text_arabic":"وَيَتَجَنَّبُهَا الْأَشْقَى  الَّذِي يَصْلَى النَّارَ الْكُبْرَى","surah_id":87,"covers_ayahs":"11-12"},{"ayah_number":13,"text_arabic":"ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَى","surah_id":87},{"ayah_number":14,"text_arabic":"قَدْ أَفْلَحَ مَنْ تَزَكَّى","surah_id":87},{"ayah_number":15,"text_arabic":"وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّى","surah_id":87},{"ayah_number":16,"text_arabic":"بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا","surah_id":87},{"ayah_number":17,"text_arabic":"وَالْآخِرَةُ خَيْرٌ وَأَبْقَى","surah_id":87},{"ayah_number":18,"text_arabic":"إِنَّ هَذَا لَفِي الصُّحُفِ الْأُولَى","surah_id":87},{"ayah_number":19,"text_arabic":"صُحُفِ إِبْرَاهِيمَ وَمُوسَى","surah_id":87}]