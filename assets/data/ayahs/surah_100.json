[{"ayah_number":1,"text_arabic":"وَالْعَادِيَاتِ ضَبْحًا","surah_id":100},{"ayah_number":2,"text_arabic":"فَالْمُورِيَاتِ قَدْحًا","surah_id":100},{"ayah_number":3,"text_arabic":"فَالْمُغِيرَاتِ صُبْحًا","surah_id":100},{"ayah_number":4,"text_arabic":"فَأَثَرْنَ بِهِ نَقْعًا","surah_id":100},{"ayah_number":5,"text_arabic":"فَوَسَطْنَ بِهِ جَمْعًا","surah_id":100},{"ayah_number":6,"text_arabic":"إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ  وَإِنَّهُ عَلَى ذَلِكَ لَشَهِيدٌ  وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ","surah_id":100,"covers_ayahs":"6-8"},{"ayah_number":9,"text_arabic":"أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ","surah_id":100},{"ayah_number":10,"text_arabic":"وَحُصِّلَ مَا فِي الصُّدُورِ","surah_id":100},{"ayah_number":11,"text_arabic":"إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ","surah_id":100}]