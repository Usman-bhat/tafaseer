[{"ayah_number":1,"text_arabic":"أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ","surah_id":107},{"ayah_number":2,"text_arabic":"فَذَلِكَ الَّذِي يَدُعُّ الْيَتِيمَ  وَلَا يَحُضُّ عَلَى طَعَامِ الْمِسْكِينِ","surah_id":107,"covers_ayahs":"2-3"},{"ayah_number":4,"text_arabic":"فَوَيْلٌ لِلْمُصَلِّينَ  الَّذِينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَ","surah_id":107,"covers_ayahs":"4-5"},{"ayah_number":6,"text_arabic":"الَّذِينَ هُمْ يُرَاءُونَ","surah_id":107},{"ayah_number":7,"text_arabic":"وَيَمْنَعُونَ الْمَاعُونَ","surah_id":107}]