[{"ayah_number":1,"text_arabic":"إِنَّا أَنْزَلْنَاهُ فِي لَيْلَةِ الْقَدْرِ","surah_id":97},{"ayah_number":2,"text_arabic":"وَمَا أَدْرَاكَ مَا لَيْلَةُ الْقَدْرِ","surah_id":97},{"ayah_number":3,"text_arabic":"لَيْلَةُ الْقَدْرِ خَيْرٌ مِنْ أَلْفِ شَهْرٍ","surah_id":97},{"ayah_number":4,"text_arabic":"تَنَزَّلُ الْمَلَائِكَةُ وَالرُّوحُ فِيهَا بِإِذْنِ رَبِّهِمْ مِنْ كُلِّ أَمْرٍ","surah_id":97},{"ayah_number":5,"text_arabic":"سَلَامٌ هِيَ حَتَّى مَطْلَعِ الْفَجْرِ","surah_id":97}]