[{"ayah_number":1,"text_arabic":"لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ  وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ","surah_id":75,"covers_ayahs":"1-2"},{"ayah_number":3,"text_arabic":"أَيَحْسَبُ الْإِنْسَانُ أَلَّنْ نَجْمَعَ عِظَامَهُ  بَلَى قَادِرِينَ عَلَى أَنْ نُسَوِّيَ بَنَانَهُ","surah_id":75,"covers_ayahs":"3-4"},{"ayah_number":5,"text_arabic":"بَلْ يُرِيدُ الْإِنْسَانُ لِيَفْجُرَ أَمَامَهُ","surah_id":75},{"ayah_number":6,"text_arabic":"يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ","surah_id":75},{"ayah_number":7,"text_arabic":"فَإِذَا بَرِقَ الْبَصَرُ  وَخَسَفَ الْقَمَرُ  وَجُمِعَ الشَّمْسُ وَالْقَمَرُ  يَقُولُ الْإِنْسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ","surah_id":75,"covers_ayahs":"7-10"},{"ayah_number":11,"text_arabic":"كَلَّا لَا وَزَرَ","surah_id":75},{"ayah_number":12,"text_arabic":"إِلَى رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ","surah_id":75},{"ayah_number":13,"text_arabic":"يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ","surah_id":75},{"ayah_number":14,"text_arabic":"بَلِ الْإِنْسَانُ عَلَى نَفْسِهِ بَصِيرَةٌ","surah_id":75},{"ayah_number":15,"text_arabic":"وَلَوْ أَلْقَى مَعَاذِيرَهُ","surah_id":75},{"ayah_number":16,"text_arabic":"لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ","surah_id":75},{"ayah_number":17,"text_arabic":"إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ","surah_id":75},{"ayah_number":18,"text_arabic":"فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ","surah_id":75},{"ayah_number":19,"text_arabic":"ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ","surah_id":75},{"ayah_number":20,"text_arabic":"كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ  وَتَذَرُونَ الْآخِرَةَ","surah_id":75,"covers_ayahs":"20-21"},{"ayah_number":22,"text_arabic":"وُجُوهٌ يَوْمَئِذٍ نَاضِرَةٌ","surah_id":75},{"ayah_number":23,"text_arabic":"إِلَى رَبِّهَا نَاظِرَةٌ","surah_id":75},{"ayah_number":24,"text_arabic":"وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ  تَظُنُّ أَنْ يُفْعَلَ بِهَا فَاقِرَةٌ","surah_id":75,"covers_ayahs":"24-25"},{"ayah_number":26,"text_arabic":"كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ","surah_id":75},{"ayah_number":27,"text_arabic":"وَقِيلَ مَنْ رَاقٍ","surah_id":75},{"ayah_number":28,"text_arabic":"وَظَنَّ أَنَّهُ الْفِرَاقُ","surah_id":75},{"ayah_number":29,"text_arabic":"وَالْتَفَّتِ السَّاقُ بِالسَّاقِ","surah_id":75},{"ayah_number":30,"text_arabic":"إِلَى رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ","surah_id":75},{"ayah_number":31,"text_arabic":"فَلَا صَدَّقَ وَلَا صَلَّى  وَلَكِنْ كَذَّبَ وَتَوَلَّى  ثُمَّ ذَهَبَ إِلَى أَهْلِهِ يَتَمَطَّى","surah_id":75,"covers_ayahs":"31-33"},{"ayah_number":34,"text_arabic":"أَوْلَى لَكَ فَأَوْلَى  ثُمَّ أَوْلَى لَكَ فَأَوْلَى","surah_id":75,"covers_ayahs":"34-35"},{"ayah_number":36,"text_arabic":"أَيَحْسَبُ الْإِنْسَانُ أَنْ يُتْرَكَ سُدًى","surah_id":75},{"ayah_number":37,"text_arabic":"أَلَمْ يَكُ نُطْفَةً مِنْ مَنِيٍّ يُمْنَى","surah_id":75},{"ayah_number":38,"text_arabic":"ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّى","surah_id":75},{"ayah_number":39,"text_arabic":"فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنْثَى  أَلَيْسَ ذَلِكَ بِقَادِرٍ عَلَى أَنْ يُحْيِيَ الْمَوْتَى","surah_id":75,"covers_ayahs":"39-40"}]