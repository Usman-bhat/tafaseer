[{"ayah_number":1,"text_arabic":"وَالضُّحَى  وَاللَّيْلِ إِذَا سَجَى","surah_id":93,"covers_ayahs":"1-2"},{"ayah_number":3,"text_arabic":"مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَى","surah_id":93},{"ayah_number":4,"text_arabic":"وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَى","surah_id":93},{"ayah_number":5,"text_arabic":"وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَى","surah_id":93},{"ayah_number":6,"text_arabic":"أَلَمْ يَجِدْكَ يَتِيمًا فَآوَى","surah_id":93},{"ayah_number":7,"text_arabic":"وَوَجَدَكَ ضَالًّا فَهَدَى","surah_id":93},{"ayah_number":8,"text_arabic":"وَوَجَدَكَ عَائِلًا فَأَغْنَى","surah_id":93},{"ayah_number":9,"text_arabic":"فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ","surah_id":93},{"ayah_number":10,"text_arabic":"وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ","surah_id":93},{"ayah_number":11,"text_arabic":"وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ","surah_id":93}]