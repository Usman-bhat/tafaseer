[{"ayah_number":1,"text_arabic":"اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ","surah_id":96},{"ayah_number":2,"text_arabic":"خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ","surah_id":96},{"ayah_number":3,"text_arabic":"اقْرَأْ وَرَبُّكَ الْأَكْرَمُ  الَّذِي عَلَّمَ بِالْقَلَمِ","surah_id":96,"covers_ayahs":"3-4"},{"ayah_number":5,"text_arabic":"عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ","surah_id":96},{"ayah_number":6,"text_arabic":"كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَى","surah_id":96},{"ayah_number":7,"text_arabic":"أَنْ رَآهُ اسْتَغْنَى","surah_id":96},{"ayah_number":8,"text_arabic":"إِنَّ إِلَى رَبِّكَ الرُّجْعَى","surah_id":96},{"ayah_number":9,"text_arabic":"أَرَأَيْتَ الَّذِي يَنْهَى  عَبْدًا إِذَا صَلَّى","surah_id":96,"covers_ayahs":"9-10"},{"ayah_number":11,"text_arabic":"أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَى  أَوْ أَمَرَ بِالتَّقْوَى","surah_id":96,"covers_ayahs":"11-12"},{"ayah_number":13,"text_arabic":"أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّى","surah_id":96},{"ayah_number":14,"text_arabic":"أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَى","surah_id":96},{"ayah_number":15,"text_arabic":"كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ  نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ  فَلْيَدْعُ نَادِيَهُ  سَنَدْعُ الزَّبَانِيَةَ","surah_id":96,"covers_ayahs":"15-18"},{"ayah_number":19,"text_arabic":"كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ","surah_id":96}]